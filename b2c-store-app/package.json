{
  "name": "b2c-store-app",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "pnpm run open-browser && turbo run dev",
    "open-browser": "start http://localhost:3000",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
